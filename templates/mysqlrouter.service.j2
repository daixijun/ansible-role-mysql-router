# {{ ansible_managed }}
[Unit]
Description=MySQL Router
After=syslog.target
After=network.target

[Service]
Type=simple
PIDFile=/var/run/mysqlrouter.pid
ExecStart=/usr/local/mysqlrouter/bin/mysqlrouter --config /usr/local/mysqlrouter/mysqlrouter.conf
User=mysqlrouter
Group=mysqlrouter
PrivateTmp=true

[Install]
WantedBy=multi-user.target
